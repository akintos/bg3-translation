#. NodeID = Objective
#. HandlePath = Description
#. ObjectiveID = ORI_COM_Shadowheart_GoToHoG
#. QuestID = ORI_COM_ShadowHeart
#: Mods/GustavDev/Story/Journal/objective_prototypes.lsx:8569
msgctxt "hb756b73cgb2b5g4802g8100g274a6d474db4"
msgid "Journey to the House of Grief."
msgstr "슬픔의 집으로 이동하자."

#. FilePath = Mods/GustavDev/Story/Journal/quest_prototypes.lsx
#. NodeID = QuestStep
#. HandlePath = Description
#. ID = SharPath_LearnedCultLocation
#. Objective = ORI_COM_Shadowheart_GoToHoG
#: Mods/GustavDev/Story/Journal/quest_prototypes.lsx:2478
