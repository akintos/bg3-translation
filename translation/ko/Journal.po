#. QuestID = LOW_FindBhaalTemple_SUB_FreeVictim
#: Mods/GustavDev/Story/Journal/objective_prototypes.lsx:6534
msgctxt "h41d9841cgab6bg4e73g8b40gb4c65f922162"
msgid "Check up on the victim."
msgstr "희생자의 상태를 확인하자."

#. FilePath = Mods/GustavDev/Story/Journal/quest_prototypes.lsx
#. NodeID = QuestStep
#. HandlePath = Description
#. ID = DeadVictim
#. Objective = LOW_FindBhaalTemple_SUB_FreeVictim_COMPLETION
#: Mods/GustavDev/Story/Journal/quest_prototypes.lsx:58984
msgctxt "h5bf0ce7dgf6c2g4d5cgad17g597697b58309"
msgid "We failed to save Orin's victim from her clutches."
msgstr "오린에게 잡혀간 희생자를 구하지 못했다."

#. FilePath = Mods/GustavDev/Story/Journal/quest_prototypes.lsx
