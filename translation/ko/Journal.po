#. QuestID = ORI_COM_Wyll_SUB_Wyrmway
#: Mods/GustavDev/Story/Journal/objective_prototypes.lsx:13242
msgctxt "h6bcf47eag1e8ag490fgbd92gfe8626ecd60a"
msgid "Find the Wyrmway."
msgstr "고룡의 길을 찾자."

#. FilePath = Mods/GustavDev/Story/Journal/quest_prototypes.lsx
#. NodeID = QuestStep
#. HandlePath = Description
#. DevComment = Entered the Wyrmway with prior knowledge
#. ID = Reached_Knows
