msgid "Half-Drow"
msgstr "하프 드로우"

#. uuid = 4fa13243-199d-4c9a-55b4-44d86a27f598
#. tag_name = DROWHALFELF
#. categories = Code, Dialog, Race, PlayerRace, CharacterSheet
