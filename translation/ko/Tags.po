msgid "Cleric of Yondalla"
msgstr "욘달라의 클레릭"

#. uuid = dc005135-7eee-4406-6ea4-359541e72224
#. tag_name = CLERIC_YONDALLA
#. categories = Dialog, Class, CharacterSheet
#. 
#. type = display_description
