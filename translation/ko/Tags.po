msgctxt "hae70879agd7f7g4281ga84cgf942e5f07a64"
msgid "Cleric of Vlaakith"
msgstr "블라키스의 클레릭"

#. uuid = 91b2d344-fce8-48b3-3da8-8896ddcbb8b1
#. tag_name = CLERIC_VLAAKITH
#. categories = Dialog, Class, CharacterSheet
