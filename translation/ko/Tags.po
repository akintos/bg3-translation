msgctxt "h3fdf92d5gcfa4g4c18g9fd0g0d823cc2d0d5"
msgid "Drow"
msgstr "드로우"

#. UUID = 676e854b-d74b-44f3-6383-f23e64714da5
#. 
#. NodeID = Tags
#. HandlePath = DisplayDescription
#. Description = |Drow, shapeshifted or not|
#. Name = REALLY_DROWELF
