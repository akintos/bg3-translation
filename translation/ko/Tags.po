msgid "Cleric of Oghma"
msgstr "오그마의 클레릭"

#. uuid = 9fbbedb6-c485-4c6b-b794-d7c72259b323
#. tag_name = CLERIC_OGHMA
#. categories = Dialog, Class, CharacterSheet
#. 
#. type = display_description
