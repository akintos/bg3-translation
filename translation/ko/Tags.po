msgctxt "h7fd943bbge764g47b2gb6c5g2ad3bd9c44d2"
msgid "Half-Drow"
msgstr "하프 드로우"

#. uuid = 3dbe23e0-2c9f-4a81-86b5-6eecf750e120
#. tag_name = REALLY_DROWHALFELF
#. categories = Code, Dialog, Race, PlayerRace
#. 
