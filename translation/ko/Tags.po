msgid "High Half-Elf"
msgstr "하이 하프 엘프"

#. uuid = 52b71dea-9d4e-402d-0097-9cfb0a36c944
#. tag_name = HIGHHALFELF
#. categories = Code, Dialog, Race, PlayerRace, CharacterSheet
#. 
#. type = display_description
