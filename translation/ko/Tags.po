msgctxt "hba15336dg4566g4f35g974bg7d4cdf993c26"
msgid "Paladin of Laduguer"
msgstr "러더거의 팔라딘"

#. uuid = 39cb63f2-3036-4641-439b-7377c934abff
#. tag_name = PALADIN_LADUGUER
#. categories = Dialog, Class, CharacterSheet
#. 
