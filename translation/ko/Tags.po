msgid "Oathbreaker"
msgstr "맹세파기자"

#. uuid = c7bdcdf2-15e7-456e-f1ad-dd2117a3182e
#. tag_name = OATHBREAKER
#. categories = Code, Dialog, Story, Class, CharacterSheet
#. 
