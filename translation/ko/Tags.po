msgid "Half-Elf"
msgstr "하프 엘프"

#. uuid = 34317158-8e6e-45a2-1ebd-04662fd8a2dd
#. tag_name = HALFELF
#. categories = Code, Dialog, Race, PlayerRace, CharacterSheet
