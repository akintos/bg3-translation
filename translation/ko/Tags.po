msgid "Cleric of Tempus"
msgstr "템푸스의 클레릭"

#. uuid = 31d96353-a4b8-4abf-ce9c-a9f74e17da65
#. tag_name = CLERIC_TEMPUS
#. categories = Dialog, Class, CharacterSheet
#. 
