msgctxt "hd8533064g0806g4274gb35cge82f653cd5c3"
msgid "Paladin of Ilmater"
msgstr "일메이터의 팔라딘"

#. uuid = bd987761-1952-4942-f185-53d44280f3af
#. tag_name = PALADIN_ILMATER
#. categories = Dialog, Class, CharacterSheet
