msgid "Mephistopheles Tiefling"
msgstr "메피스토펠레스 티플링"

#. uuid = c70e1dc2-72d4-44f9-fd83-3dc67e249bdd
#. tag_name = REALLY_MEPHISTOPHELESTIEFLING
#. categories = Code, Dialog, Race, PlayerRace
