#: Shared/Public/Shared/Tags/3ce8734c-440c-43f8-b0f4-079d69e7eb33.lsx:8
msgctxt "hbe4de057g8105g420fg9d83gf02a15a12d3b"
msgid "Cleric of Moradin"
msgstr "모라딘의 클레릭"

#. uuid = 3ce8734c-440c-43f8-f4b0-9d07e76933eb
#. tag_name = CLERIC_MORADIN
#. categories = Dialog, Class, CharacterSheet
#. 
#. type = display_description
