msgid "Paladin of Helm"
msgstr "헬름의 팔라딘"

#. uuid = 36c5df8b-7efc-4125-8e82-0e12cfd671fa
#. tag_name = PALADIN_HELM
#. categories = Dialog, Class, CharacterSheet
