msgid "Half-Elf"
msgstr "하프 엘프"

#. uuid = 277f9609-ad0c-4883-6db5-50084e90df88
#. tag_name = REALLY_HALFELF
#. categories = Code, Dialog, Race, PlayerRace
#. 
#. type = display_description
