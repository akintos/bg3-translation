msgctxt "h367a12c4g533dg4b2egb9c5gec4908107c40"
msgid "<i>Attack the brain anyway.</i>"
msgstr "<i>아무튼 뇌를 공격한다.</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer
#. Speaker = Us
#. NodeID = 9af4a6a8-6e86-7899-8179-d6855b4550c2
