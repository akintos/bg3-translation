msgctxt "hb138acc0g85b8g4445g8514gfabf15ff8821"
msgid "<i>We shall be as one.</i>"
msgstr "<i>우리는 하나가 될 것이다.</i>"

#. Dialog = Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback
#. Speaker = GROUP_Animals
