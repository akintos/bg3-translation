msgctxt "h97069c26g765dg4b2egac52gc35633b7f997"
msgid "*Bloomridge Park ahead.*"
msgstr "*블름릿지 파크가 앞에 있습니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_GeneralCityStreets_AD_Sign_StreetDirections_03
#. Speaker = Narrator
#. NodeID = 9af26671-bddc-22d1-6c92-74da21ab9f4c
