msgctxt "h26c67ac2g9ab6g44fcg9d61gf8fbbc26645a"
msgid "Inappropriate."
msgstr "뭐 하는 짓이지."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[4].TagText[2]
#. TagRule: REALLY_WYLL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "heeded996g9c3dg4733gb1e4gd748b21d9c8e"
msgid "Do I need to keep my good eye on you?"
msgstr "내가 널 두고봐야 하나?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[5].TagText[0]
#. TagRule: REALLY_KARLACH
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h37654ac3gc255g4024g8240g450003216dff"
msgid "Hey now! What's that all about?"
msgstr "저기! 뭐 하는 짓이야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[5].TagText[1]
#. TagRule: REALLY_KARLACH
