#.      - 0f049027-3d60-502d-9b25-8629c3897d37_END_EmperorPrelude_Local_A = True
#. SetFlags:
#.   Local:
#.     - 0f049027-3d60-502d-9b25-8629c3897d37_END_EmperorPrelude_Local_A = False
#. TagRule: REALLY_SHADOWHEART
#: Mods/GustavDev/Story/Dialogs/Global/GLO_EmperorPrelude_PAD_BoatBlocked:4267a4c5-0058-4d1e-9420-ce89486ce9fd
msgctxt "h1ce00befg5643g44a8g9e3fg579ba2ef18e6"
msgid "Abandoned. Wonder why."
msgstr "버려졌군. 어째서일까."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_EmperorPrelude_PAD_BoatBlocked
#. Speaker = Player
#. NodeID = 4267a4c5-0058-4d1e-9420-ce89486ce9fd
