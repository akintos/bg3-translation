msgctxt "h2f6336c2ged7ag4e57ga484g7469db74653f"
msgid "Hide all you like, I'll find you! "
msgstr "얼마든지 숨어봐, 내가 찾아내고 말겠다! "

#. Dialog = Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback
#. Speaker = GROUP_Animals
#. NodeID = 2e581b8c-642e-43f1-98ae-2342b8812f12
#. HandlePath = TaggedText[0].TagText[1]
#: Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback:2e581b8c-642e-43f1-98ae-2342b8812f12
msgctxt "h20169b66gf376g4350gb032g824fe2460dc6"
msgid "Where the hells have they gone? "
msgstr "대체 어디로 간 거야? "

#. Dialog = Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback
#. Speaker = GROUP_Animals
#. NodeID = 2e581b8c-642e-43f1-98ae-2342b8812f12
#. HandlePath = TaggedText[0].TagText[2]
#: Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback:2e581b8c-642e-43f1-98ae-2342b8812f12
msgctxt "h17b9b661gd6b5g40bfg8bd2g086d9679e01f"
msgid "I know you're still out there! "
msgstr "아직 거기 밖에 있는 거 알아! "

#. Dialog = Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback
#. Speaker = GROUP_Animals
#. NodeID = 2e581b8c-642e-43f1-98ae-2342b8812f12
#. HandlePath = TaggedText[0].TagText[3]
