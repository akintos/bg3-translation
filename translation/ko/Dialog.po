#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_11:4f751237-91f7-825f-7666-e12fc9fa9afc
msgctxt "hc2d9a4f0gbc04g4f97gbab9g4f744c503978"
msgid ""
"*Adventurers Alexer and Orion Goodbeard - Intrepid explorers of the Plane of "
"Shadows.*"
msgstr "*모험가 알렉서와 오리온 구드비어드 - 그림자 평원의 용감한 모험가들.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_12
#. Speaker = Narrator
#. NodeID = 78660c99-3b14-a737-c177-556cd958eaba
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_12:78660c99-3b14-a737-c177-556cd958eaba
msgctxt "h0059e259g2ee8g4e0fgb700gc279cb5acf45"
msgid "*This tomb has been defaced with crude marks pointing downward.*"
msgstr "*이 무덤은 아래를 향하는 이상한 표식들로 훼손되어 있습니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_13
#. Speaker = Narrator
#. NodeID = 58eb9100-7f23-8d75-3642-4fdc5410c477
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_13:58eb9100-7f23-8d75-3642-4fdc5410c477
msgctxt "h248a6ae3g5a71g493dgbf7bg1fd154a3acd5"
msgid "*Josy Ann Fountainhead. Outstanding Manip of the Fists.*"
msgstr "*조시 앤 파운틴헤드. 플레이밍피스트의 촉망받던 부사관.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_14
#. Speaker = Narrator
#. NodeID = 108eef7f-bf13-7289-bb89-9948d8dfc5c2
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_14:108eef7f-bf13-7289-bb89-9948d8dfc5c2
msgctxt "hfdd0d335g4882g4250gb4f2g8d4e658b0638"
msgid ""
"*This tombstone has been vandalised, and now reads: 'Your money don't matter "
"when you're dead, prick'.*"
msgstr "*이 묘비는 훼손되어, 이렇게 적혀있습니다. '죽으면 돈이 무슨 소용이야, 바보 자식'*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_15
#. Speaker = Narrator
#. NodeID = bdf5d9f3-c445-1be4-87b3-396aa23374f0
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_15:bdf5d9f3-c445-1be4-87b3-396aa23374f0
msgctxt "h08e770f6g791fg417cg81d9g424e47d3a3eb"
msgid ""
"*This stone on this vandalised tomb reads: 'Cry a rain of tears so flowers "
"may bloom over Iris'.*"
msgstr "*훼손된 묘비의 돌 위엔 '눈물이 비가 되어 아이리스를 꽃피우다.'라고 적혀있습니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_16
#. Speaker = Narrator
#. NodeID = 17f5b36b-1940-ba9e-36c0-d6765dabd966
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_16:17f5b36b-1940-ba9e-36c0-d6765dabd966
msgctxt "h623f198cg6797g491bgac5eg01db4fb698fb"
msgid ""
"*Bibeek, favoured of Oghma. Your songs live on in the streets and in our "
"hearts.*"
msgstr "*오그마의 충신 비빅. 당신의 노래는 언제나 거리와 우리 마음속에 살아있을 거예요.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_17
#. Speaker = Narrator
#. NodeID = 48fbd68c-b566-037d-63ea-6a8196f2f1a3
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_17:48fbd68c-b566-037d-63ea-6a8196f2f1a3
msgctxt "hbf09565dga559g4540g9a21gf790a629cb35"
msgid ""
"*The stone on this vandalised tomb now reads: 'Fear the Lord of Bones. He "
"never dies'.*"
msgstr "*훼손된 묘비의 돌 위엔 이제 '뼈의 주인을 두려워하라. 그는 절대 죽지 않으니'라 적혀있다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_18
#. Speaker = Narrator
#. NodeID = e4634ca4-6bd7-e389-5203-aef025dd9370
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_18:e4634ca4-6bd7-e389-5203-aef025dd9370
msgctxt "ha68866cbg052cg4f92gb05dg9cf6390cf836"
msgid ""
"*Honoured Commander Harold Loggerson. Killed by deceitful followers of "
"Bhaal. We will not let this happen again.*"
msgstr "*존경받는 지휘관 아놀드 로거슨. 바알 광신도의 속임수로 사망함. 다른 이들에겐 이런 일이 일어나지 않도록 할게.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_19
#. Speaker = Narrator
#. NodeID = 35bdd317-a3c4-8c9b-710f-f38de3c7e5ee
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_19:35bdd317-a3c4-8c9b-710f-f38de3c7e5ee
msgctxt "h14bca6fbg8b6cg4d8dg92ebg7ae811494984"
msgid "*Mighty Branwem. The Foehammer grants you honour in death.*"
msgstr "*마이티 브랜웸. 적과의 싸움 중 명예롭게 사망*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_20
#. Speaker = Narrator
#. NodeID = d68bfc14-5e99-d184-7bcd-eb0461c1de0a
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_20:d68bfc14-5e99-d184-7bcd-eb0461c1de0a
msgctxt "h72e8abf3g637fg4691g9c77gd6db1d9706a5"
msgid "*Gwynn Highberry - Stillborn but Not Forgotten*"
msgstr "*그윈 하이베리 - 세상에 나오지도 못한 우리 아이 널 잊지 못할 거야*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_21
#. Speaker = Narrator
#. NodeID = 2703fbe4-cdaf-c9a5-b7d9-82381cf396dd
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_21:2703fbe4-cdaf-c9a5-b7d9-82381cf396dd
msgctxt "hf3fd1c1ag8a7dg4b3fg8299gcc8dd692c754"
msgid "*Antunio Bongle - Proud Father of Wulbren, Remembered by the Son*"
msgstr "*안투니오 봉글 - 울브렌의 자랑스러운 아버지, 아들에 의해 영원토록 기억되리.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_22
#. Speaker = Narrator
#. NodeID = e3a5801d-e6ee-6750-c950-d8a84e76a27a
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_22:e3a5801d-e6ee-6750-c950-d8a84e76a27a
msgctxt "h100de6b6gee0eg4b70ga9b2g68596d060666"
msgid "*Fidia Penghyst - A Noble Lanceboard Player to the Last*"
msgstr "*피디아 펭기스트, 마지막까지 고결했던 랜스보드 플레이어.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_23
#. Speaker = Narrator
#. NodeID = 0f5a3170-e595-21a8-6137-9803fd708081
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_23:0f5a3170-e595-21a8-6137-9803fd708081
msgctxt "hc7290a28g78f0g41dfgbd05g01c270fe689c"
msgid ""
"*Here lies Guyira 'Snakemother' Denysse, matriarch to a brood of venomous "
"daughters*"
msgstr "*독 자매단의 수장, '뱀 어머니' 기이라 데니스 여기 잠들다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_24
#. Speaker = Narrator
#. NodeID = 1cb0ff8c-001f-172d-af2e-ce217b5b9cfc
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_24:1cb0ff8c-001f-172d-af2e-ce217b5b9cfc
msgctxt "h2cb977fbg4b49g40c3ga714g42e87b6edb34"
msgid ""
"*Grint Thorg, Fighter, Lost in Durlag's Tower. We couldn't recover your body "
"so we buried your ten-foot pole.*"
msgstr "*전사 그린트 토르그, 둘라그 타워에서 실종. 시신을 수습하지 못해 대신 그가 쓰던 10피트 막대를 대신 묻음.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_25
#. Speaker = Narrator
#. NodeID = 734522aa-0702-1928-256b-b1c1993a68db
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_25:734522aa-0702-1928-256b-b1c1993a68db
msgctxt "h3df032feg6286g4debg9ed4g4badf292a88b"
msgid "*Here lies Flying Martha, who couldn't*"
msgstr "*이름과 달리 날지 못했던 플라잉 마사, 여기 잠들다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_26
#. Speaker = Narrator
#. NodeID = e5e71445-9f20-f9a9-0fb7-98d2a753abe2
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_26:e5e71445-9f20-f9a9-0fb7-98d2a753abe2
msgctxt "hdd584c98g745fg4ea1gb60cga869cf626b1f"
msgid "*Flandro 'Tarrasque-Biter' Grassley - The hunter hunted, the biter bit*"
msgstr "*'타라스크 깨물기' 플란드로 그래슬리, 본인이 사냥당하고, 본인이 깨물렸다네.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_27
#. Speaker = Narrator
#. NodeID = 100501c2-57d7-41a0-f6f8-2d2bdf63eac0
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_27:100501c2-57d7-41a0-f6f8-2d2bdf63eac0
msgctxt "h335e7b7cgd4aag4ecfg936dg05bfb3dd24d6"
msgid "*Didn't know her name, we just called her 'Gremishka'.*"
msgstr "*이름은 모르겠고 그냥 '그레미쉬카'라고 불리던 여인의 묘.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_28
#. Speaker = Narrator
#. NodeID = 287767be-6a40-4b92-a49d-5c386d44dc0d
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_28:287767be-6a40-4b92-a49d-5c386d44dc0d
msgctxt "h916ec307gf895g4144ga7cegc6d860941f85"
msgid "*Aradin the Elder - 'Open the Bloody Gates'*"
msgstr "*늙은이, 아라딘 - '관짝 뚜껑 좀 열어줘봐'*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_29
#. Speaker = Narrator
#. NodeID = b5e61807-29b1-c5c7-f119-3fa15337ec54
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_29:b5e61807-29b1-c5c7-f119-3fa15337ec54
msgctxt "h6d0fda07ga416g4829g9d14g68c81142e134"
msgid "*Here lies Anna-Marie, my nanny but good as mum.*"
msgstr "*이곳에 아나 마리가 뉘어있다. 나의 유모, 동시에 친모와도 같았던.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_30
#. Speaker = Narrator
#. NodeID = 604aaa5d-e9b4-21d1-4c04-50aa1cc3c4e0
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_30:604aaa5d-e9b4-21d1-4c04-50aa1cc3c4e0
msgctxt "had352995gf030g4dd7gbe63gaaaa883a4245"
msgid "*Here lies Bargus Freely. He should've lookt before he leapt.*"
msgstr "*여기 바르거스 프릴리가 잠들어있다. 이 녀석아. 밑은 보고 뛰었어야지.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_31
#. Speaker = Narrator
#. NodeID = cf87885d-c7c3-e2b8-cf45-d7800e6b235e
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_31:cf87885d-c7c3-e2b8-cf45-d7800e6b235e
msgctxt "h7546548fga4aag40cbg945dg5012f5889e11"
msgid ""
"*Here lies Ogre Bob (his arm, anyway, it's all that would fit). He will be "
"missed at Whitburn Quarry.*"
msgstr "*여기 오우거 밥이 뉘어있다. (엄밀히 말하자면 팔만, 그 이상은 안 들어갔음). 휘트번 채석장에서 그를 그리워 할 것임.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_32
#. Speaker = Narrator
#. NodeID = 2f0898eb-121e-8cb6-806c-3050d5529c10
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_32:2f0898eb-121e-8cb6-806c-3050d5529c10
msgctxt "he6db0d59g1192g4211gbf0bgdd8c41d85711"
msgid "*Here lies anonymous. Whoever you were, we buried you with care.*"
msgstr "*여기 누군가 잠들어 있습니다. 당신이 누군였는지 모르겠지만 성심성의껏 묻어드렸습니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_33
#. Speaker = Narrator
#. NodeID = 4b6dc700-b72a-ba69-ec3a-70ef447cbccd
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_33:4b6dc700-b72a-ba69-ec3a-70ef447cbccd
#, fuzzy
msgctxt "h72881898g76dbg43f5ga28agbf2caf1e62f7"
msgid "*One more hapless farmer boy, name unknown.*"
msgstr "*운 지지리도 없는 이름 모를 농부 소년의 묘.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_34
#. Speaker = Narrator
#. NodeID = 015d7056-bcf3-bfed-60c4-2615bd0f1d4c
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_Tombstone_34:015d7056-bcf3-bfed-60c4-2615bd0f1d4c
msgctxt "h94208c3fgebedg4686gaefdg676786580edd"
msgid "*Here lies Duncan, last of the Ghorey clan, who died without issue.*"
msgstr "*고리 일족의 마지막 혈통 던컨, 여기에 고이 잠들다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_WorkshopTombstone
#. Speaker = Narrator
#. NodeID = 68430d39-fbe0-fd35-9e46-a99fe11fce8c
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Graveyard_AD_WorkshopTombstone:68430d39-fbe0-fd35-9e46-a99fe11fce8c
msgctxt "h85643187g6330g427fg8203g31dd4ce9e4bf"
msgid "*This tombstone is yet to be engraved.*"
msgstr "*이 묘비는 아직 새겨지지 않았습니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Guildhall_AD_Rule
#. Speaker = Narrator
#. NodeID = 5263c286-eaf1-aa13-f44b-84fdac6a78e0
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Guildhall_AD_Rule:5263c286-eaf1-aa13-f44b-84fdac6a78e0
msgctxt "hfee0b4abg07c2g421cg890cgdb26c475dc42"
msgid ""
"*The Guildhall has one rule: All crime stays in the city streets, where it "
"belongs. Break this rule and you die.*"
msgstr "*길드회관의 단 한가지 규칙. '모든 범죄는 도시 거리에서만, 어길시 사형.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Guildhall_AD_VintageMoonshaeZinfandelCollection
#. Speaker = Narrator
#. NodeID = 3add563c-a33a-b0e3-537d-6a5708c4a356
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_Guildhall_AD_VintageMoonshaeZinfandelCollection:3add563c-a33a-b0e3-537d-6a5708c4a356
msgctxt "hcee91582g72c0g4c65gaf62gc8e3f89267b4"
msgid ""
"*Someone's Vintage Moonshae Zinfandel Collection. But it looks like a lot of "
"the bottles have gone missing.*"
msgstr "*누군가의 문셰이 진판델 수집품. 그러나 꽤나 많은 병들이 사라진 것 같다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_HarbourviewParkthesmallerone_AD_TrackRepairs
#. Speaker = Narrator
#. NodeID = f8f8f8e5-20a3-095a-c64c-dd73c1b09c51
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_HarbourviewParkthesmallerone_AD_TrackRepairs:f8f8f8e5-20a3-095a-c64c-dd73c1b09c51
msgctxt "he81e2964g87b1g44b0gbbb9ge3d88b10fbb4"
msgid "*Tracks undergoing repair - Enter this area at your own risk*"
msgstr "*수리 중인 트랙 - 진입하는 순간 순전히 본인의 책임입니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_HouseOfHope_AD_DevilishNaturmort
#. Speaker = Narrator
#. NodeID = dbc4f6a3-da46-7664-a404-57f0b2d581c4
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_HouseOfHope_AD_DevilishNaturmort:dbc4f6a3-da46-7664-a404-57f0b2d581c4
msgctxt "h5a50b264gf736g4a58g9ee8g42dbc49c4aa0"
msgid ""
"*In the twisted strokes of the artist's brush, you sense the fragments of a "
"forsaken soul laid out in reds, blues, and yellows.*"
msgstr "*당신은 예술가의 뒤틀린 붓질에서 버려진 영혼의 파편이 붉은색, 푸른색, 노란색으로 펼쳐지는 것을 느낍니다.*"

#. Dialog = Mods/GustavDev/Story/Dialogs/GeneratedNarratorADs/LOW_IronThrone_AD_DoNotTauntTheSpectators
#. Speaker = Narrator
#. NodeID = 6178747f-7a50-a1ab-2431-519bebd26ff3
#. HandlePath = TaggedText[0].TagText[0]
