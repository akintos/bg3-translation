msgctxt "h7f2158d9gb076g4471ga309g1e69389c6455"
msgid "<i>Return to us, child.</i>"
msgstr "<i>우리에게 돌아오거라, 아이야.</i>"

#. Dialog = Mods/Shared/Story/Dialogs/Generics/NO_RECORD/CMB_AD_SeekFallback
