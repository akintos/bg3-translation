msgctxt "h75ebc19dg0a55g4ef0gb93eg731f12226cf7"
msgid "Won't budge."
msgstr "꿈쩍도 안 해."

#. Dialog = Mods/Shared/Story/Dialogs/Generics/GEN_AD_ItemBlocked
#. Speaker = Player
#. NodeID = 8921ab3d-f738-a4ce-e727-95de80e089c6
#. HandlePath = TaggedText[0].TagText[2]
#: Mods/Shared/Story/Dialogs/Generics/GEN_AD_ItemBlocked:8921ab3d-f738-a4ce-e727-95de80e089c6
msgctxt "h4dbce797g6ef6g4acdgb32dge84e7fbd728d"
msgid "No use. It's blocked."
msgstr "소용 없어. 막혔어."

#. Dialog = Mods/Shared/Story/Dialogs/Generics/GLO_AD_ActivatedElsewhere
