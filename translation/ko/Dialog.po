msgctxt "h50b9688fg60c7g498fgbe38g15fea035f2fd"
msgid "<i>Leave.</i>"
msgstr "<i>떠난다.</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl
#. Speaker = Lae'zel
#. NodeID = 4e6d8429-002e-5602-9e37-801c02c4b994
