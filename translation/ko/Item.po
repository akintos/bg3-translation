#. Type = item
#: Public/Shared/RootTemplates/_merged.lsx:299745
msgctxt "hfcf7d3d4gf346g4a6bga2a0g536bbb4429fc"
msgid "Raphael in Molten Shadow"
msgstr "녹아내리는 그림자 속 라파엘"

#. FilePath = Public/Shared/RootTemplates/_merged.lsx
#. NodeID = GameObjects
#. HandlePath = Description
#. Icon = Item_FUR_GEN_Painting_Portrait_R
