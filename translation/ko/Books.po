msgid "Confidant of a Duke"
msgstr ""

#. key = LOW_Golbraith_CorrespondenceWithAbdelAdrien
#. 
#. BookId = LOW_Golbraith_CorrespondenceWithAbdelAdrien
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
