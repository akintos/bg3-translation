msgid "Masha's Wedding Vows"
msgstr ""

#. key = LOW_BhaalTemple_MashasWeddingVows
#. 
#. BookId = LOW_BhaalTemple_MashasWeddingVows
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
