msgctxt "hde28ada1g2be8g47cdg9ca3g655be6fb9144"
msgid "Web-Covered Journal"
msgstr "거미줄로 뒤덮힌 일지"

#. BookId = FOR_Bottomless_HermitBook4
#. 
#. NodeID = GameObjects
#. HandlePath = Description
#. Name = UNI_FOR_Bottomless_HermitBook4
#. Type = item
