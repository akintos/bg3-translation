msgid "Opportunity to work with a master artisan"
msgstr ""

#. key = LOW_GeneralLowerCity_JobSeeker
#. 
#. BookId = LOW_GeneralLowerCity_JobSeeker
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
