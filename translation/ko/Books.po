msgid "Open Call for Adventurers on Behalf of the Society"
msgstr ""

#. key = LOW_TheLodge_EggContract
#. 
#. BookId = LOW_TheLodge_EggContract
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
