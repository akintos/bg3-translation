msgid "Glory of the Absolute"
msgstr ""

#. key = MOO_MoonriseTowers_GloryOfTheAbsolute
#. 
#. BookId = MOO_MoonriseTowers_GloryOfTheAbsolute
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
