msgctxt "h0333fadbg0f32g48dbg817bgb1a86cf471ae"
msgid "Schools of Magic: Conjuration"
msgstr "마법 학파: 조형계"

#. key = LOW_SorcerousSundries_SchoolsOfMagic2Conjuration
#. 
#. BookId = LOW_SorcerousSundries_SchoolsOfMagic2Conjuration
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
