msgid "Orcs of the Trielta Hills"
msgstr ""

#. key = MOO_OrcsOfTheTrieltaHills
#. 
#. BookId = MOO_OrcsOfTheTrieltaHills
#. 
#. FilePath = Mods/GustavDev/Localization/Act2_Books.lsx
