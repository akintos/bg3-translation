msgid "Concerning Orin the Red"
msgstr ""

#. key = LOW_BhaalTemple_ConcerningOrinTheRed
#. 
#. BookId = LOW_BhaalTemple_ConcerningOrinTheRed
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
