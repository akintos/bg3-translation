msgid "Iron Throne - Windup and Shutdown"
msgstr ""

#. key = LOW_IronThrone_WindupAndShutdown
#. 
#. BookId = LOW_IronThrone_WindupAndShutdown
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
