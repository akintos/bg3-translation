msgctxt "hf2fb793bgfd2ag4319g8975g8567591f19fc"
msgid "Flask of Light"
msgstr "빛의 플라스크"

#. key = BOOK_Alchemy_LightFlask
#. 
#. BookId = BOOK_Alchemy_LightFlask
#. 
#. FilePath = Mods/GustavDev/Localization/Act1b_Books.lsx
