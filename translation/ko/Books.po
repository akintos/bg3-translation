msgid "Counsellor Florrick's Sentence"
msgstr ""

#. key = LOW_BasiliskGate_FlorrickSentence
#. 
#. BookId = LOW_BasiliskGate_FlorrickSentence
#. 
#. FilePath = Mods/GustavDev/Localization/Act3_Books.lsx
