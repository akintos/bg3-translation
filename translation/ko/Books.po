msgid "URGENT - Bhaalist Resurgence"
msgstr ""

#. key = LOW_BasiliskGate_DevellasRequest
#. 
#. BookId = LOW_BasiliskGate_DevellasRequest
#. 
#. FilePath = Mods/GustavDev/Localization/Act3_Books.lsx
#. NodeID = TranslatedStringKey
#. HandlePath = Content
