msgid "Request to Join the Hag Survivors"
msgstr ""

#. key = LOW_AuntieEthelsRevenge_JatloLetter
#. 
#. BookId = LOW_AuntieEthelsRevenge_JatloLetter
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
