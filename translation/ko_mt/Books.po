msgctxt "h604a60aeg2906g4b2eg9f42gdc6d87489152"
msgid "Songs of the Wind"
msgstr "바람의 노래"

#. key = LOW_SorcerousSundries_BookByElminster2
#. 
#. BookId = LOW_SorcerousSundries_BookByElminster2
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
#. NodeID = TranslatedStringKey
#. HandlePath = Content
