msgctxt "h10a67236g91a5g48efgb84cg726bcc187cd9"
msgid "The Unburdening"
msgstr "짐 내려놓기"

#. key = LOW_SharranCloister_UnburdeningProcess
#. 
#. BookId = LOW_SharranCloister_UnburdeningProcess
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
