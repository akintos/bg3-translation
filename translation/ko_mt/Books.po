msgid "Larethian"
msgstr "라레시안"

#. key = GLO_GeneralLoreBook_CorellonLarethian
#. 
#. BookId = GLO_GeneralLoreBook_CorellonLarethian
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
