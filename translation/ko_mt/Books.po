msgid "Kythorn 1477"
msgstr "카이톤 1477"

#. key = LOW_CazadorsPalace_Kythorn1477
#. 
#. BookId = LOW_CazadorsPalace_Kythorn1477
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
