msgid "Mirtul 1477"
msgstr "머털 1477"

#. key = LOW_CazadorsPalace_Mirtul1477
#. 
#. BookId = LOW_CazadorsPalace_Mirtul1477
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
