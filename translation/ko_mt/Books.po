msgid "Recipe for Fiddlehead Soup"
msgstr "어린잎 수프 요리법"

#. key = LOW_Elfsong_FiddleheadSoup
#. 
#. BookId = LOW_Elfsong_FiddleheadSoup
#. 
#. FilePath = Mods/GustavDev/Localization/Act3_Books.lsx
