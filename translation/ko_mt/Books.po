#. key = GEN_OrinsMessages_SteelWatchFoundryDestroyed
#. 
#. BookId = GEN_OrinsMessages_SteelWatchFoundryDestroyed
#. 
#. FilePath = Mods/GustavDev/Localization/Act3_Books.lsx
