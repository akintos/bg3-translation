msgid "Stolen Chickens"
msgstr "훔친 닭"

#. key = LOW_BasiliskGate_AngryLetter
#. 
#. BookId = LOW_BasiliskGate_AngryLetter
#. 
#. FilePath = Public/GustavDev/Localization/generated_books.lsx
