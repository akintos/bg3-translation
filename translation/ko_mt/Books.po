msgctxt "h72b6a14bg247eg4982g8d71g493aea00dd91"
msgid "Patriar Family Dossiers"
msgstr "귀족 가문 서류"

#. key = LOW_CountingHouse_Vault8_BlackmailFiles
#. 
#. BookId = LOW_CountingHouse_Vault8_BlackmailFiles
#. 
#. FilePath = Mods/GustavDev/Localization/Act3_Books.lsx
