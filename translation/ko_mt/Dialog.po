msgctxt "h40ffb26bg6b72g406dg831fg837ebb575673"
msgid "<i>Attack.</i>"
msgstr "<i>공격한다.</i>"

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_Myconids_Trespass
#. Speaker = Player
#. NodeID = c4af28e1-57a2-4fbb-6aca-7d96d9a057d7
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/Shared/Story/Dialogs/Crimes/NGB_Myconids_Trespass:c4af28e1-57a2-4fbb-6aca-7d96d9a057d7
msgctxt "h71fd5ab5g1534g45dega6e8ge8ba39ebd123"
msgid "Sorry. I'm on my way."
msgstr "미안, 그냥 가 볼게."

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_Myconids_Trespass
#. Speaker = Narrator
#. NodeID = f11aff51-1465-4b77-959b-403dbaffc6be
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/Shared/Story/Dialogs/Crimes/NGB_Myconids_Trespass:f11aff51-1465-4b77-959b-403dbaffc6be
msgctxt "h1210a4e3g12cfg4ee3gb4a8g54600b63b1b2"
msgid "*The creature approaches. This ground is forbidden to your kind.*"
msgstr "*마이코니드가 다가옵니다. 여긴 당신 같은 종족에겐 금지된 곳이라는군요.*"

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention
#. Speaker = Narrator
#. NodeID = e326c916-b5a8-0934-33ca-a47a0d224f1a
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention:e326c916-b5a8-0934-33ca-a47a0d224f1a
msgctxt "hfa447cb1g2444g4b3cga267g0d00b92fae26"
msgid "*Your actions have attracted some attention.*"
msgstr "*당신의 행동이 주의를 끌게 됐습니다.*"

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention
#. Speaker = Narrator
#. NodeID = e326c916-b5a8-0934-33ca-a47a0d224f1a
#. HandlePath = TaggedText[0].TagText[1]
#: Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention:e326c916-b5a8-0934-33ca-a47a0d224f1a
msgctxt "h5cbda24cgc389g48cag836cga6d090c804e6"
msgid "*You are about to be investigated for that.*"
msgstr "*좀 전의 행위로 인해 조사받게 될 겁니다.*"

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention
#. Speaker = Narrator
#. NodeID = e326c916-b5a8-0934-33ca-a47a0d224f1a
#. HandlePath = TaggedText[0].TagText[2]
#: Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention:e326c916-b5a8-0934-33ca-a47a0d224f1a
msgctxt "h4025e334g3b19g48dagbe0dg9b9d25f1ed1b"
msgid "*You'd better have a good explanation for what you just did.*"
msgstr "*왜 그런 짓을 했는지 설명할 수 있어야 할 겁니다.*"

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_PAD_AttractedAttention
#. Speaker = Narrator
#. NodeID = e326c916-b5a8-0934-33ca-a47a0d224f1a
