msgctxt "h8f0cc701g6db2g4c95gba33gcc995d996e93"
msgid "<i>Leave.</i>"
msgstr "<i>자리를 뜬다.</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodLock
#. Speaker = Player
#. NodeID = 49c33d5b-d010-5434-bc89-1657c7746c5c
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodLock:49c33d5b-d010-5434-bc89-1657c7746c5c
msgctxt "hcfbd450bg6f81g49cfgaefdgb219ebf870ec"
msgid "<i>Leave.</i>"
msgstr "<i>자리를 뜬다.</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodLock
#. Speaker = Narrator
#. NodeID = 55674fa0-fd37-eb3f-e5fa-baa28b6b6ce6
#. HandlePath = TaggedText[0].TagText[0]
