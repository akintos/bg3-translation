msgctxt "h8ab97cb3gb310g482eg8709g367ebbac7a80"
msgid "<i>Really?</i>"
msgstr "<i>진짜?</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[2].TagText[1]
#. TagRule: REALLY_GALE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hda0707e9gd6d7g491bga10dg80edfdf553c6"
msgid "Stop it!"
msgstr "하지 마!"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[2].TagText[2]
#. TagRule: REALLY_GALE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h4f1196cfg039dg44bega619g1ec7d1961956"
msgid "Get off!"
msgstr "저리 가!"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[3].TagText[0]
#. TagRule: REALLY_SHADOWHEART
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hbd2f1952g3e22g43e0g8625gb8e37abf5beb"
msgid "Stop that!"
msgstr "그만 해!"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[4].TagText[0]
#. TagRule: REALLY_WYLL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "haf305b01g1e96g4528ga6edgbae33aa2fb9d"
msgid "I hope that was an honest mistake."
msgstr "정말 실수였길 바라지."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[4].TagText[1]
#. TagRule: REALLY_WYLL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h26c67ac2g9ab6g44fcg9d61gf8fbbc26645a"
msgid "Inappropriate."
msgstr "이건 아니지."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[4].TagText[2]
#. TagRule: REALLY_WYLL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "heeded996g9c3dg4733gb1e4gd748b21d9c8e"
msgid "Do I need to keep my good eye on you?"
msgstr "내가 계속 지켜봐야겠어?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[5].TagText[0]
#. TagRule: REALLY_KARLACH
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h37654ac3gc255g4024g8240g450003216dff"
msgid "Hey now! What's that all about?"
msgstr "이봐! 대체 왜 그러는 거야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[5].TagText[1]
#. TagRule: REALLY_KARLACH
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h8b6563bcg1b66g46bcg98a2g6f4af21f289c"
msgid "Easy does it, soldier."
msgstr "그만해, 병사."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[5].TagText[2]
#. TagRule: REALLY_KARLACH
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h34c2ec3cg0a8dg47afg934cgfe0b4cfbf01e"
msgid "C'mon now, knock that off."
msgstr "제발, 그만 좀 해."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[6].TagText[0]
#. TagRule: REALLY_JAHEIRA
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h8ab67f7bg0166g4610g9270gbc6bb45d22a4"
msgid ""
"I would not like to end our acquaintance in violence. But that does not mean "
"I won't."
msgstr "난 이 관계를 폭력으로 끝내고 싶지 않아. 하지만 어쩔 수 없다면 나도 못 참아."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[7].TagText[0]
#. TagRule: REALLY_HALSIN
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hdd9242f5g8431g421cgb6a9gac4a74fde135"
msgid "You would wrong an ally? Shameful."
msgstr "동료를 위험하게 하다니. 부끄러운 줄 아시오."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[8].TagText[0]
#. TagRule: REALLY_MINTHARA
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h5f68643dg1cf7g4c77ga2fcg302a4f280c27"
msgid "Cross me again, and I will end you."
msgstr "자꾸 성질 건드리면 끝장을 내주겠어."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[9].TagText[0]
#. TagRule: REALLY_MINSC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h2a931dd3g2cb8g49e4g81a5g9ed735e877c0"
msgid "HEY! <i>Hands off the hamster.</i>"
msgstr "야! <i>그 햄스터 건드리지 마.</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[10].TagText[0]
#. TagRule: BARD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h56e37788gb73eg45d9ga2d4gc60396d022d9"
msgid ""
"I feel the urge to perform in the middle of the night - right by your "
"bedroll."
msgstr "한밤중에 네 침대 옆에서 연주하고 싶은 충동이 느껴지네."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[10].TagText[1]
#. TagRule: BARD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hbca3afefg904cg4696g88f4gf64a31f11531"
msgid "Fun fact - musical instruments can also be used to murder."
msgstr "재미있는 게 뭔지 알아? 악기로 사람을 죽일 수도 있다는 거야."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[10].TagText[2]
#. TagRule: BARD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hdee4eb72g79b7g45f3g96b2g0d6b221f8ce8"
msgid ""
"I've come up with a ballad about you  - it's called the  'Festering Worm'"
msgstr "너에 대한 곡을 써 봤어. 제목은 \"썩은 벌레\"야."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[11].TagText[0]
#. TagRule: BARBARIAN
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h61be8f8fg8da3g4521gb6a9g806cea6c3d8f"
msgid "I'd break you in two... if you were worth it."
msgstr "똑바로 좀 해, 둘로 쪼개버리기 전에."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[11].TagText[1]
#. TagRule: BARBARIAN
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h81b4c8c8g05cfg4529g9a5bg518e26296851"
msgid "Move along, little bug."
msgstr "좀 비켜, 벌레야 뭐야."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[11].TagText[2]
#. TagRule: BARBARIAN
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hadb4c8a1g4e1dg4719gbca7ge845e089f19a"
msgid "You really don't want to make me angry."
msgstr "나 열받게 하려고 일부러 그러는 건 아니지?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[11].TagText[3]
#. TagRule: BARBARIAN
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h1ba7370eg6556g41dfgbe5agf919f35e0d01"
msgid "<i>ROAAAAR!</i>"
msgstr "<i>으아아아!</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[12].TagText[0]
#. TagRule: CLERIC_GOOD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h7f0c0390g6ce8g4f23g834fg06f4641dadfd"
msgid "I am blessed to be here - stop making me question that."
msgstr "난 여기 있는 게 좋아. 의심하게 만들지 마."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[12].TagText[1]
#. TagRule: CLERIC_GOOD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "haec9a3beg6e48g479cg9295g69aa65f65789"
msgid "You are fortunate I worship one of the kinder gods of the realm."
msgstr "내가 아주 자비로운 신을 섬기는 게 다행인 줄 알아."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[12].TagText[2]
#. TagRule: CLERIC_GOOD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h2819b46bg033eg47begb81dgafe47de8119f"
msgid "Grant me patience..."
msgstr "참자... 참아야 해..."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[13].TagText[0]
#. TagRule: CLERIC_NEUTRAL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h806c5f4cgf7d3g4360gaba0g7243eb8d5fe1"
msgid "I worship a god who cares not for you. Remember that. "
msgstr "내가 믿는 신은 널 아끼지 않아. 명심해. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[13].TagText[1]
#. TagRule: CLERIC_NEUTRAL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h1d6100e9gc8c3g4bc7gb1ddgb4bc09fa6eea"
msgid "If you've been sent to test me - it's working."
msgstr "날 시험하려고 한 짓이라면... 성공했네."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[13].TagText[2]
#. TagRule: CLERIC_NEUTRAL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h86392000gc202g45bdg91e5g299fa7ade05a"
msgid "Grant me patience..."
msgstr "참자... 참아야 해..."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[14].TagText[0]
#. TagRule: CLERIC_EVIL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "had2b2f0cg86aag4827g88a3g8c61a2ed8c36"
msgid "You do not wish to irritate me - or the one I serve."
msgstr "내 믿음과 인내심을 시험하지 말아줘."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[14].TagText[1]
#. TagRule: CLERIC_EVIL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h26c47bccgacf6g464fg9223gdae7815f14a7"
msgid "If you're going to torment me, at least try harder."
msgstr "날 괴롭히고 싶으면 더 열심히 해봐."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[14].TagText[2]
#. TagRule: CLERIC_EVIL
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "he9f5ba22gc4b8g43a3gb9e7g4c6ca6002e65"
msgid "Away, <i>insect.</i>"
msgstr "저리 가, <i>벌레</i> 같으니."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[15].TagText[0]
#. TagRule: FIGHTER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h0eacd597gfc3cg465bg9d52ga2dedadb30da"
msgid "Enough of that - we need to be in fighting shape."
msgstr "그만 좀 해. 싸울 준비를 해야지."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[15].TagText[1]
#. TagRule: FIGHTER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h8e48df90g22d4g4c10gaed9g63222242d388"
msgid "Remind me why I allied with you?"
msgstr "내가 왜 널 동료로 맞이했는지 알긴 해?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[15].TagText[2]
#. TagRule: FIGHTER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h598d1b3aged2eg4022g9876ge0df35749500"
msgid "I'll be at your back in our next fight. Remember that."
msgstr "다음 전투에선 내가 네 뒤에 있을게. 명심해."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[16].TagText[0]
#. TagRule: WIZARD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hbee93ff8g9891g4ba1g9853g47da2cbfeddc"
msgid "Bold actions for someone in fireball distance. "
msgstr "화염구에 맞고 싶으면 어디 마음대로 해 봐. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[16].TagText[1]
#. TagRule: WIZARD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h5ef985d1g69c3g47dbga4a7g096f7813c0bb"
msgid "If I could cast Wish right now, you'd be an ant forever. "
msgstr "지금 당장 소원을 시전할 수 있다면 널 영원히 개미로 만들겠어. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[16].TagText[2]
#. TagRule: WIZARD
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h230cc8f6gfbebg48e8g9b99gc9bac38d8899"
msgid "It is unwise to irritate someone who can <i>command the Weave.</i>"
msgstr "<i>위브</i>를 지배할 수 있는 자를 자극하는 건 현명하지 않아."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[17].TagText[0]
#. TagRule: WARLOCK
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h9277571fg4693g454dgb56dg84f26bdc7e23"
msgid "I made a pact for great power - only to end up here. With you."
msgstr "강력한 힘을 위해 계약을 맺었는데, 너와 이러고 있는 신세라니."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[17].TagText[1]
#. TagRule: WARLOCK
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h9cf8cceeg34afg4adagab2fg1ac192ef53d3"
msgid "I call upon you, gracious patron, to rid me of this worm. "
msgstr "은혜로운 후견자시여, 이 벌레를 제거해 주소서. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[17].TagText[2]
#. TagRule: WARLOCK
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hf836e7b6g019dg4f96gace3g35ae0996ec9f"
msgid "I wonder... Could I make a pact to rid myself of you?"
msgstr "혹시... 널 없애는 계약을 맺을 수는 없을까?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[18].TagText[0]
#. TagRule: DRUID or PALADIN_ANCIENTS or RANGER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hbbdad41bg1eb7g41degadc6g87853f6784fc"
msgid "Do not incur nature's wrath, fool. "
msgstr "자연의 분노가 두렵지도 않아? 멍청이. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[18].TagText[1]
#. TagRule: DRUID or PALADIN_ANCIENTS or RANGER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h070a4ff0g268dg44f2gb8e7g8ece2cb26d0b"
msgid "Nature can be both kind - and cruel. Remember that."
msgstr "자연은 친절할 수도, 잔인할 수도 있어. 기억해 둬."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[18].TagText[2]
#. TagRule: DRUID or PALADIN_ANCIENTS or RANGER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h812ce70fg9010g415dg94a7g5ab7223de0bc"
msgid "You're being a real thorn in my side."
msgstr "너 정말 눈엣가시가 되고 있어."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[19].TagText[0]
#. TagRule: MONK
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hc8c7ad4fg3187g4a57ga913gfacbaa7e1a27"
msgid "Such childishness does not become allies."
msgstr "그렇게 유치하게 굴면 동료가 될 수 없어."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[19].TagText[1]
#. TagRule: MONK
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h4d457dc0g281dg4022g8d7fg55bde1059eeb"
msgid "What, exactly, was the point of that?"
msgstr "그래서, 정확하게 요점이 뭐야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[19].TagText[2]
#. TagRule: MONK
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "ha6411ffdgf492g48a1gb500g91d926a2ebf1"
msgid "I am trained to embody eternal patience. You are pissing me off. "
msgstr "끝없이 인내할 수 있도록 훈련받았어. 그런데 네가 날 열받게 하고 있네. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[20].TagText[0]
#. TagRule: SORCERER_WILDMAGIC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h240e0eb7ge539g4914gb923g6427ba7bbd7f"
msgid "You know I can set you, and possibly myself, on fire, right?"
msgstr "잊어버린 모양인데, 내가 널 불태워 버릴 수 있다는 거 알지?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[20].TagText[1]
#. TagRule: SORCERER_WILDMAGIC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h643e1cc9g3182g44a4g8e4agf0159f357f8d"
msgid "Don't make me use my magic - not even I know what will happen."
msgstr "내가 마법을 사용하게 만들지 마. 무슨 일이 일어날지 나도 장담 못 하니까."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[20].TagText[2]
#. TagRule: SORCERER_WILDMAGIC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "he24059c4g6a0fg4f72g937bga123011a3139"
msgid "I accidentally summoned an imp like you once."
msgstr "전에도 실수로 너 같은 임프를 소환한 적이 있지."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[21].TagText[0]
#. TagRule: SORCERER_DRACONIC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h17a420f4g17c5g4565ga7eeg185c08d3ca08"
msgid "Stop being such a pain in the scales."
msgstr "왜 그렇게 골칫거리가 되려고 애쓰는 거야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[21].TagText[1]
#. TagRule: SORCERER_DRACONIC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h7acb8f0dg6e6eg4e34g9b6ag43a9e75ea9a9"
msgid "If my ancestors were here, they would eat you alive - literally."
msgstr "내 조상들이 여기 있었다면 널 산 채로 잡아먹었을 거야. 정말로."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[21].TagText[2]
#. TagRule: SORCERER_DRACONIC
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h738f31cageef7g466dgb7b1gd8491085c28c"
msgid "Don't make me burn you to a crisp."
msgstr "바삭하게 태워 버리기 전에 그만둬."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[22].TagText[0]
#. TagRule: ROGUE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h63ea00c8g94e4g4eddga8d6gc9aa5ca7e120"
msgid "You know I can stab you in your sleep, right?"
msgstr "네가 자는 동안 내가 찌를 수도 있다는 거 알지?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[22].TagText[1]
#. TagRule: ROGUE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h7526a325g91aag4813g8a7bgbfc73679a132"
msgid "Wyvern, Assassin's blood - oh, I'm just listing all the poisons I know."
msgstr "와이번의 독, 암살자의 피... 아, 내가 아는 독이 뭐가 있나 떠올려 봤어."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[22].TagText[2]
#. TagRule: ROGUE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hfb1aad4eg594bg471fga735g11aa691e800d"
msgid "There are sixteen different ways I could really hurt you right now. "
msgstr "널 괴롭힐 수 있는 방법을 세 봤더니 16가지가 있네. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[23].TagText[0]
#. TagRule: PALADIN_DEVOTION
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h7cbf7cd3g4c25g434cgbb0eg0de494b716f8"
msgid "You tempt me to break my oath, whelp. "
msgstr "맹세를 지켜야 해서 참는 거야. 아직까지는. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[23].TagText[1]
#. TagRule: PALADIN_DEVOTION
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hd9d3031fg42c1g424bg9b8ag03200c7a9ce9"
msgid "I fight for justice - and will not hesitate to execute it."
msgstr "정의를 위해서라면 주저하지 않고 행동으로 옮길 거야."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[23].TagText[2]
#. TagRule: PALADIN_DEVOTION
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hf1005718g1651g43faga50bge8d9df05d12d"
msgid "Stray from the path, and I will correct you."
msgstr "선을 넘기만 해 봐. 내가 바로잡아 주지."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[24].TagText[0]
#. TagRule: PALADIN_VENGEANCE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h5d63c602gbd15g4b09ga986gba1550a74998"
msgid "You do <i>not </i>wish to incur my vengeance. I guarantee you. "
msgstr "내가 어떻게 복수하는지 알면 이러지 <i>못할</i> 텐데. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[24].TagText[1]
#. TagRule: PALADIN_VENGEANCE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h10183359gf2b4g4ca1gacdfgb679bf4dda56"
msgid "I punish evil and minor annoyances equally. "
msgstr "날 귀찮게 하는 것들은 악마와 똑같이 처벌하지. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[24].TagText[2]
#. TagRule: PALADIN_VENGEANCE
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "ha852ab21gbdb4g4348g9df1ga284e9ff01db"
msgid "I am not known for my <i>patience.</i>"
msgstr "내가 원래 <i>인내심</i>이 없는 편이라서."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[25].TagText[0]
#. TagRule: OATHBREAKER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h3a512cb4ga11eg4e64g805ag5698c2cf114e"
msgid "I have already broken my oath. I'll happily break you as well."
msgstr "난 이미 맹세를 깼으니, 너도 기꺼이 깨줄 수 있어."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[25].TagText[1]
#. TagRule: OATHBREAKER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h5ece9011gc9feg4a85g8bf2gd3406c755026"
msgid "Do you wish for death?"
msgstr "죽고 싶은 거야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[25].TagText[2]
#. TagRule: OATHBREAKER
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h4f2346c3gbc37g41f0ga777ga84d55014ea8"
msgid "I am not known for my <i>patience.</i>"
msgstr "내가 원래 <i>인내심</i>이 없는 편이라서."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[26].TagText[0]
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "h5454fb19gf356g48d3g9e22gac8280e3390c"
msgid "Do you mind?"
msgstr "정말 이럴 거야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[26].TagText[1]
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hbcbe5fe9gfb31g4a64g82cfg53b6fc5e6336"
msgid "Stop that."
msgstr "그만."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[26].TagText[2]
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hefb48e47ge800g4387g8425g7c3e235f1366"
msgid "Enough already."
msgstr "그만해."

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[26].TagText[3]
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hfa8de922g29c3g4a70gb22bg948a9d4048e4"
msgid "My patience is wearing. "
msgstr "인내심이 바닥나고 있어. "

#. Dialog = Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam
#. Speaker = Player
#. NodeID = 6ad94e76-a4a5-40eb-846d-e8141e3594f9
#. HandlePath = TaggedText[26].TagText[4]
#: Mods/Gustav/Story/Dialogs/Generics/GLO_PAD_CrimeAgainstPartOfTheTeam:6ad94e76-a4a5-40eb-846d-e8141e3594f9
msgctxt "hf117a130gfaacg453dg9480gd3bafe5dbd34"
msgid "Surely you have <i>something</i> better to do. "
msgstr "고작 <i>이 정도밖에</i> 안 되는 놈이었어? "

#. Dialog = Mods/Shared/Story/Dialogs/Generics/GLO_PAD_HagGruel
#. Speaker = Player
#. NodeID = c4b8ae50-76de-40d2-8fab-a4192b8a15b7
#. HandlePath = TaggedText[0].TagText[0]
#. TagRule: REALLY_ASTARION
#: Mods/Shared/Story/Dialogs/Generics/GLO_PAD_HagGruel:c4b8ae50-76de-40d2-8fab-a4192b8a15b7
msgctxt "h026aead2gd760g4753g8a1bgd8c1432b511c"
msgid "That is <i>foul</i>."
msgstr "도저히 <i>못 봐주겠군</i>."

#. Dialog = Mods/Shared/Story/Dialogs/Generics/GLO_PAD_HagGruel
#. Speaker = Player
