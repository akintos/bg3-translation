msgctxt "ha5bb833fgeadbg4b80ga865g939997ddb461"
msgid "Cuckold-courting curmudgeon!"
msgstr "구두쇠에 바람피울 상이로군!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 6096b776-605d-7688-6daa-a22d753fc729
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:6096b776-605d-7688-6daa-a22d753fc729
msgctxt "h1f6c6281geb42g4ac1gabd0gcfa786e0b20c"
msgid "You act a fighter, but a fool's a fool."
msgstr "센 척하는데, 머저리는 머저리일 뿐."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 611a8526-5ad4-1230-8222-597f6a8b6b1a
