msgctxt "h96da78ebgd497g492bg9027gb6e0d294ecbd"
msgid "Beautiful playing. "
msgstr "아름다운 곡이야. "

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/GEB_AD_MusicalPerformance_Good
#. Speaker = GROUP_Civilians
#. NodeID = ddf336a1-123e-74ce-7da0-fd5249bc7308
#. HandlePath = TaggedText[0].TagText[1]
#: Mods/Shared/Story/Dialogs/Crimes/GEB_AD_MusicalPerformance_Good:ddf336a1-123e-74ce-7da0-fd5249bc7308
msgctxt "h9f567c0bg1259g40cbgab69gcce1312ce3ad"
msgid "I've heard <i>much</i> worse."
msgstr "이런 명연주는 <i>처음 들어.</i>"

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/GEB_AD_MusicalPerformance_Good
#. Speaker = GROUP_Civilians
#. NodeID = ddf336a1-123e-74ce-7da0-fd5249bc7308
#. HandlePath = TaggedText[0].TagText[2]
#: Mods/Shared/Story/Dialogs/Crimes/GEB_AD_MusicalPerformance_Good:ddf336a1-123e-74ce-7da0-fd5249bc7308
msgctxt "ha4da7ad8gb544g4023gb4c3g23d3ba555cbc"
