msgctxt "hb6996147g31a1g4accgbe89g74ac4a6066de"
msgid "*Nothing.*"
msgstr "*봐도 모르겠군요.*"

#. Dialog = Mods/Gustav/Story/Dialogs/Global/GLO_InfernalBox_BoxInteraction
#. Speaker = Astarion
#. NodeID = 2c0b01dd-f02b-1b21-805e-6f19a316c716
