msgctxt "h3b8cc32eg6e60g4c61g8043g412f6caadb7c"
msgid "Pus-supping maggot."
msgstr "고름 먹는 구더기로군."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 0ae2a6e8-9f87-7874-28bf-e990cdecb7b5
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:0ae2a6e8-9f87-7874-28bf-e990cdecb7b5
msgctxt "h063e335ega97bg437dgba0bg154b27d08239"
msgid "Away, you pox'd nun!"
msgstr "꺼져라, 병자 새끼야!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 0ebef6f1-dd97-40ab-f42c-7c39e0898b7f
#. HandlePath = TaggedText[0].TagText[0]
