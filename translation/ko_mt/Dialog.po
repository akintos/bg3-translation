msgstr "이게 무슨 조화야? 그 애물단지를 우리더러 떠맡으라고?"

#. Dialog = Mods/Gustav/Story/Dialogs/Global/GLO_InfernalBox_BoxInteraction
#. Speaker = Player
#. NodeID = 74132751-21d5-9dca-66ae-80a65a558408
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/Gustav/Story/Dialogs/Global/GLO_InfernalBox_BoxInteraction:74132751-21d5-9dca-66ae-80a65a558408
msgctxt "h372369d7g0b76g4ddag8740g721b26973af5"
msgid "<i>Put it away.</i>"
msgstr "<i>저리 치운다.</i>"

#. Dialog = Mods/Gustav/Story/Dialogs/Global/GLO_InfernalBox_BoxInteraction
#. Speaker = Lae'zel
#. NodeID = 7476a65e-e895-85c4-efe4-2926b36e19ec
