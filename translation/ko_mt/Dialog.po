#.     - CRIME_FoundGuiltyPlayer(0) = True
#.     - CRIME_CallGuardsFromDialog(0) = True
#: Mods/Shared/Story/Dialogs/Crimes/NGB_Interrogation_DisarmingTrap:54d0ad95-4963-4725-ac1a-b078f861e38f
msgctxt "h049411a1g3e13g42aega6afgf1c7a565bc94"
msgid ""
"*You make an unconvincing attempt to talk your way out of this. Perhaps "
"you'll have more luck with the guards.*"
msgstr ""

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_Interrogation_DisarmingTrap
#. Speaker = Narrator
#. NodeID = 54d0ad95-4963-4725-ac1a-b078f861e38f
#. HandlePath = TaggedText[0].TagText[1]
#. CheckFlags:
#.   Tag:
#.      - CIVILIAN(0) = True
#.      - BADASSCIVILIAN(0) = False
#. SetFlags:
#.   Object:
#.     - CRIME_FoundGuiltyPlayer(0) = True
#.     - CRIME_CallGuardsFromDialog(0) = True
#: Mods/Shared/Story/Dialogs/Crimes/NGB_Interrogation_DisarmingTrap:54d0ad95-4963-4725-ac1a-b078f861e38f
msgctxt "he95139c4ga3bfg485aga048gac989312d9f1"
msgid ""
"*Perhaps the guards will find your words to be of interest. They served "
"little purpose here.*"
msgstr ""

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_Interrogation_DisarmingTrap
#. Speaker = Narrator
#. NodeID = 54d0ad95-4963-4725-ac1a-b078f861e38f
#. HandlePath = TaggedText[0].TagText[2]
#. CheckFlags:
#.   Tag:
#.      - CIVILIAN(0) = True
#.      - BADASSCIVILIAN(0) = False
#. SetFlags:
#.   Object:
#.     - CRIME_FoundGuiltyPlayer(0) = True
#.     - CRIME_CallGuardsFromDialog(0) = True
#: Mods/Shared/Story/Dialogs/Crimes/NGB_Interrogation_DisarmingTrap:54d0ad95-4963-4725-ac1a-b078f861e38f
msgctxt "h6a8b3a95gd2d9g421fg9479g1fe8c25c6986"
msgid ""
"*Try as you might, your efforts are not convincing enough. The guards will "
"deal with this.*"
msgstr ""

#. Dialog = Mods/Shared/Story/Dialogs/Crimes/NGB_Interrogation_DisarmingTrap
#. Speaker = Narrator
#. NodeID = 54d0ad95-4963-4725-ac1a-b078f861e38f
#. HandlePath = TaggedText[0].TagText[3]
#. CheckFlags:
#.   Tag:
#.      - CIVILIAN(0) = True
#.      - BADASSCIVILIAN(0) = False
#. SetFlags:
