#. SetFlags:
#.   Global:
#.     - TUT_TransformChamber_State_LaezelRushed = True
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl:4e6d8429-002e-5602-9e37-801c02c4b994
msgctxt "h75018752g766fg4c5fgb529gcd3c9eb99aa3"
msgid "We have no time for stragglers."
msgstr "낙오자까지 챙길 시간 없어."

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl
#. Speaker = Player
#. NodeID = 52f4d68c-2250-f598-633f-0fa83a299a03
#. HandlePath = TaggedText[0].TagText[0]
#. CheckFlags:
#.   Dialog:
#.      - TUT_TransformChamber_PodGirl_LookforOtherWay(2) = False
#.   Object:
#.      - TUT_TransformChamber_State_HasPodKey(2) = False
#. SetFlags:
#.   Dialog:
#.     - TUT_TransformChamber_PodGirl_LookforOtherWay(2) = True
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl:52f4d68c-2250-f598-633f-0fa83a299a03
msgctxt "h049ce07dg78c7g4748g9274g246246393164"
msgid "I'll go look around - there must be some way to get this thing open."
msgstr "방법을 찾아볼게. 포획낭을 열 방법이 분명히 있을 거야."

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl
#. Speaker = Losiir
#. NodeID = 53b4db60-65f9-6a62-5882-d6e48cbac2da
#. HandlePath = TaggedText[0].TagText[0]
#. CheckFlags:
#.   Local:
#.      - c22dadaa-8b0d-277d-1bda-887ea11ab99c_TUT_MindflayerCall_GuidePresent = True
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl:53b4db60-65f9-6a62-5882-d6e48cbac2da
msgctxt "h9e14f61ag13cag4417g83cfgd6b4575b69bc"
msgid "Enough. Do we go, or do we die?"
msgstr "적당히 해. 여기서 떠날 거야, 아니면 죽을 거야?"

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodGirl
#. Speaker = Player
#. NodeID = 546802d2-40fb-dbd5-ac10-c04923d59748
#. HandlePath = TaggedText[0].TagText[0]
#. RollType: SkillCheck
#. RollAbility: Intelligence
#. DifficultyClass: 7
