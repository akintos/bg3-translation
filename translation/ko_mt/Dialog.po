msgctxt "h0a2a3e02g088ag4b9eg9520gde71b79bd5dc"
msgid "You spherical git."
msgstr "이 머저리 새끼."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[36]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hec2a2b49gc568g4a95g9a7ege21f71d99062"
msgid "Scab-licking vagrant!"
msgstr "피고름 핥는 부랑자 새끼."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[37]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "ha58663f2gfb84g4738gb6cbg1362e6191e6e"
msgid "Thou art saucy as gruel."
msgstr "거만한 것은 죽어야 낫지."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[38]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hb9af9299ga3b0g4caagb00dg3e32e696ce6e"
msgid "Away, eye-watering stench!"
msgstr "네 악취에 눈물이 다 고이네!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[39]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hb78ab125gd7fag46e8gb10ag2e52614b9670"
msgid "A gift for a loathsome neighbour."
msgstr "꼴 보기 싫은 이웃에 딱 맞는 선물이로군."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[40]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h4e311fbage834g406dgabafgc7ce8b0bde1c"
msgid "Back to the anus that spat you out. "
msgstr "널 싸지른 구멍으로 돌아가라. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[41]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h69159af1g8cafg4df1gbe73g64c3322a0a94"
msgid "Behold, a most noisy cuck!"
msgstr "보아라, 입만 살았도다!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[42]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hc0acc693gf833g4a9fga9d7g5be2c8a45459"
msgid "Feculent beast!"
msgstr "불결한 짐승!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[43]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h398133c1g9fa9g491cg8347gb2166c7fff64"
msgid "Thou pyrite-loving tit."
msgstr "금색이라면 오줌도 받아먹을 새끼."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[44]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h811671f4g9cceg4fd6g9470g9475d64f0a30"
msgid "Majesty of manure, sovereign of shit! "
msgstr "참으로 웅장한 배설물이군! "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[45]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h68110046gb58eg44d2ga9e0g75b24c73a957"
msgid "Poisonous, necrotic appendage."
msgstr "썩은 종기 같은 놈."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[46]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hc60f3cb1g858dg4fc3gae58g5d6e896066c9"
msgid "You could send an elf to sleep!"
msgstr "널 보면 엘프도 잠에 들겠군!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[47]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hf125befag0c0ag4406g9286ge8cb8763c45a"
msgid "For you, oh horned cheek. "
msgstr "얼굴인지 종기인지. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[48]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h7c08ff32gddddg4f79g82e7gb02e14ee441c"
msgid "Get thee gone, custardous discharge."
msgstr "꺼져라, 분비물 덩어리 같은 놈아."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[49]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h904796cfg75f0g4dd2g8031g34fa3f1a20a7"
msgid "You've barely brain to feed a louse."
msgstr "네 콩알만 한 뇌로는 진딧물도 못 먹이겠군."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[50]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hc95d944agf25ag4e6fg98d5g702b2afe2c8d"
msgid "Rank, malodorous pustule."
msgstr "고름 악취로는 일등감이야."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[51]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "haad7c998gcee5g45e2g8c4cg13aaced0342a"
msgid "You're greasier than a worg-roast. "
msgstr "구운 워그보다도 기름이 가득 찼어. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[52]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h9ae49361gcf2eg4c7egb5a3g529ab0f85385"
msgid "Thick as a glob of unguent. "
msgstr "멍청한 데는 약도 없을 텐데. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[53]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "he0aa173bga53eg4a13gaaebgf9bfcb59e374"
msgid "You're a vile blister - I shall prick thee. "
msgstr "잘 여문 물집 같군. 터뜨려 줘야겠어. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[54]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h6ba47664g7b8ag4917gb5b9g4f5b3a561475"
msgid "You cyst. You misshapen flesh-lump. "
msgstr "기형적인 살덩어리 혹이 말도 할 줄 아네. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[55]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h49bd4b47g08aeg4f9fg932cg463e4d88f9cb"
msgid "As the leg, you'll end in defeat. "
msgstr "네 패배는 이미 정해졌다. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[56]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h9d0bc076g94a4g455ega35cg938d9f58bc4d"
msgid "Even Hell'd gag on your bedevilled stench."
msgstr "네 악취엔 지옥도 한 수 접겠구나."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[57]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h56390d64g83eeg4ed6gb73cg2e8aaf345dff"
msgid "Which worg upchucked you?"
msgstr "워그가 먹다 뱉은 토사물인가?"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[58]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h7327688fg2fc8g4615g9765g7a28b1ad4c96"
msgid "There's a hag-pit stench about you. "
msgstr "마귀할멈 굴 냄새가 진동하는구나. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[59]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h597f44c7gcc97g45cfg96e5g624d69aa1c5e"
msgid "Barrel-sharp, the wit on you. "
msgstr "병신 짓도 참 기가 막히네. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[60]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hb91019f6g6639g42b9gaa10g744b4c953ebc"
msgid "An ode for you? <i>Odour</i>, mayhaps. "
msgstr "널 위한 시? <i>괄시</i>가 있지. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[61]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hba8abe83g47b9g4dabg8dbcg01c4a5c69a52"
msgid "Your head's empty as a debtor's purse."
msgstr "머리가 빚쟁이 지갑처럼 텅 비었구나."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[62]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hb289f06fg05e3g4705g93b5gf7c17411a1c0"
msgid "You act a fighter, but a fool's a fool."
msgstr "센 척하는데, 머저리는 머저리일 뿐."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[63]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h4c1ec81bg43afg4d8aga10dg0a9bea2287a9"
msgid "Your body's a temple - to an idiot god. "
msgstr "그 몸은 마치 얼간이 신의 신전이로다. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[64]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h3426569cg71d3g4beeg8f6dgd5c9aa5c253f"
msgid "Your breath'd kill the dead twice over."
msgstr "죽음조차도 죽일 입 냄새야."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[65]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "he72e269ega789g4237gaff8gaa17c8886745"
msgid "Pus-supping maggot."
msgstr "고름 먹는 구더기로군."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[66]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hd417e510g0197g4d95gb7a9g10129dda9b3d"
msgid "Lily-lunged limpet-luster! "
msgstr "폐병에 착 들러붙은 염증 같구나! "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[67]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h782bd460g3e4cg4283gb25egfeb380eca586"
msgid "Worg-rutting wrong-wrinkler!"
msgstr "워그에 발정난 게 잔뜩 쪼그라들었구만!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[68]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h72367401gd9b0g4ea6g8636g68ed75259aed"
msgid "Behold! Elminster's ball-bag. "
msgstr "보아라! 엘민스터의 불알주머니로구나. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[69]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "h350c5435g6ceag4729ga064gc334fe60a431"
msgid "Love-limb of a lecherous lich!"
msgstr "시체가 좋아할 할 상이로다!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b06cf95-c885-54b2-449b-3d57943e4349
#. HandlePath = TaggedText[0].TagText[70]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b06cf95-c885-54b2-449b-3d57943e4349
msgctxt "hd9559bddg1af1g49d4gbb00gdff7b49b47f8"
msgid "Poxy pissered pervert!"
msgstr "하찮은 변소 변태 새끼!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
