msgctxt "hc18ee1dag49afg4789gb563g3736d0cf3dc8"
msgid "Thou pyrite-loving tit."
msgstr "금색이라면 오줌도 받아먹을 새끼."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 08f4a028-5d02-b231-98df-d4ebe80eb4fa
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:08f4a028-5d02-b231-98df-d4ebe80eb4fa
msgctxt "h205da8e7gb7a6g4b3fgbb6cgbe8fa4047315"
msgid "Your breath'd kill the dead twice over."
msgstr "죽음조차도 죽일 입 냄새야."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 09311639-2224-eeed-bd35-e8837436b5bd
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:09311639-2224-eeed-bd35-e8837436b5bd
msgctxt "h0670bc3age90cg4715ga7ceg2b40f1978846"
msgid "Wetter than a ten-tongued tarpon!"
msgstr "턱 빠진 물고기보다 더 축축하구나!"

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
