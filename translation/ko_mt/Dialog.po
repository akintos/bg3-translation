#. TagRule: REALLY_SHADOWHEART
#: Mods/Gustav/Story/Dialogs/Global/GLO_PAD_Crate_Wood_Open_Interact:b9f8bf73-826a-437a-a963-1132627ab8d1
msgctxt "h4e6a404dgb191g4ee1g84dfgc2678ebce265"
msgid "An empty box. If I were a cat, this would be a great find."
msgstr "빈 상자네. 내가 고양이라면 좋아서 폴짝 뛸 텐데."

#. Dialog = Mods/Gustav/Story/Dialogs/Global/GLO_PAD_Crate_Wood_Open_Interact
#. Speaker = Player
#. NodeID = b9f8bf73-826a-437a-a963-1132627ab8d1
#. HandlePath = TaggedText[5].TagText[0]
#. TagRule: REALLY_WYLL
#: Mods/Gustav/Story/Dialogs/Global/GLO_PAD_Crate_Wood_Open_Interact:b9f8bf73-826a-437a-a963-1132627ab8d1
msgctxt "h76ac38e9g3f55g46c7g9d69g5a93fc917205"
msgid "Quite empty."
msgstr "텅 비었네."

#. Dialog = Mods/Gustav/Story/Dialogs/Global/GLO_PAD_Crate_Wood_Open_Interact
#. Speaker = Player
#. NodeID = b9f8bf73-826a-437a-a963-1132627ab8d1
