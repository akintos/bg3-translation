msgctxt "h4fcceeaeg7d61g4345gbb9dgd8bd0e0ec7d0"
msgid "*Perhaps it will open the nearby pod.*"
msgstr "*가동하면 옆에 있는 포획낭이 열릴지도 모르겠군요.*"

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_TransformChamber_PodLock
#. Speaker = Narrator
#. NodeID = 6a34f2d7-501e-424f-9c27-0bb62bf049cf
