#.     - GLO_TUT_Knows_TalkedToIntellectDevourer(2) = True
#. TagRule: REALLY_GITHYANKI
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer:7b51102d-f064-974e-82d1-1759f2705295
msgctxt "h0654838eg3b36g45d4gbc40g4b3654634eb0"
msgid ""
"<i>A pet of </i>ghaik <i>scum. Destroy the intellect devourer while it's "
"vulnerable.</i>"
msgstr ""

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer
#. Speaker = Player
#. NodeID = 7b51102d-f064-974e-82d1-1759f2705295
#. HandlePath = TaggedText[1].TagText[0]
#. SetFlags:
#.   Global:
#.     - TUT_Lab_State_TryKillBrain = True
#.   User:
#.     - GLO_TUT_Knows_TalkedToIntellectDevourer(2) = True
#. TagRule: REALLY_DUERGARDWARF
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer:7b51102d-f064-974e-82d1-1759f2705295
msgctxt "hc22bf5d7gcca6g4936ga165g8f7c2072d782"
msgid "<i>Kill the mind flayer slave before the corruption spreads.</i>"
msgstr ""

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer
#. Speaker = Psionic Presence
#. NodeID = 7d5af7e1-4767-28d1-78df-86b9a2f9050e
#. HandlePath = TaggedText[0].TagText[0]
#. SetFlags:
#.   Global:
#.     - TUT_Lab_Knows_BrainfreeMeans = True
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer:7d5af7e1-4767-28d1-78df-86b9a2f9050e
msgctxt "hb01c72aege986g4d3eg9133gaf4c9c87884a"
msgid "<i>Remove us from this body - from this case free us. Please!</i>"
msgstr ""

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer
#. Speaker = Psionic Presence
#. NodeID = 7e4cda5f-f405-d5b0-5fc7-bf24dfa99dd0
#. HandlePath = TaggedText[0].TagText[0]
#. SetFlags:
#.   Global:
#.     - TUT_Lab_Event_UsStartsFleeing = True
#.   Object:
#.     - TUT_Lab_EndInclusionDevourer(2) = True
#: Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer:7e4cda5f-f405-d5b0-5fc7-bf24dfa99dd0
msgctxt "h41f08c9eg8d2cg45aegb74fgf85973caef2c"
msgid "*SCREECH!*"
msgstr ""

#. Dialog = Mods/Gustav/Story/Dialogs/Tutorial/TUT_Lab_TrappedDevourer
#. Speaker = Narrator
#. NodeID = 81ac264c-e1b2-e627-2ec2-516758b5431b
