msgctxt "h43afbba4g5701g43e4gb8bag66729024777b"
msgid "Sod this for a game of soldiers."
msgstr "굼벵이 같은 새끼."

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 4b19b3bb-acd4-4433-79d3-303cffa1f20b
#. HandlePath = TaggedText[0].TagText[0]
#: Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast:4b19b3bb-acd4-4433-79d3-303cffa1f20b
msgctxt "h73fd8217gb571g46d9g84bbg6636d3f503b8"
msgid "Sharp as a pig's testicle. "
msgstr "쓸모가 있겠네. 딱 돼지 고환만큼. "

#. Dialog = Mods/GustavDev/Story/Dialogs/Global/GLO_SpellVocalComponent_ViciousMockery_Cast
#. Speaker = Player
#. NodeID = 5167440f-702e-792a-f493-1ea6a9bf76e2
#. HandlePath = TaggedText[0].TagText[0]
